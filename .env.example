# Turing Service Environment Configuration

# Base URL for the service
BASE_URL=http://localhost:8000

# External Segmentation Service (turing-segmentation microservice)
TURING_SEGMENTATION_URL=https://your-runpod-instance.com:8000
TURING_SEGMENTATION_API_KEY=your-secret-api-key-here

# Google Gemini API for AI analysis (optional)
GOOGLE_GEMINI_API_KEY=your_google_gemini_api_key_here

# Replicate API for segmentation
REPLICATE_API_TOKEN=your_replicate_api_token_here

# Development settings
DEBUG=true
LOG_LEVEL=INFO